{
  "name": "@o1labs/testnet-sdk",
  "version": "0.1.0-alpha",
  "description": "Utility for spinning up Coda testnets.",
  "bin": {
    "coda-network": "bin/coda-network"
  },
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "make-jsoo": "make -C ../.. client_sdk && cp ../../_build/default/src/app/client_sdk/client_sdk.bc.js src",
    "prepublishOnly": "yarn make-jsoo && yarn build && yarn test",
    "test": "tsc test/Test.ts && node test/Test.js"
  },
  "keywords": [
    "coda",
    "cryptocurrency"
  ],
  "author": "o1labs",
  "license": "MIT",
  "homepage": "https://codaprotocol.com",
  "repository": "https://github.com/CodaProtocol/coda-automation",
  "bugs": "https://github.com/CodaProtocol/coda-automation/issues",
  "main": "src/SDKWrapper.js",
  "types": "src/SDKWrapper.d.ts",
  "devDependencies": {
    "bs-platform": "^6.2",
    "gentype": "^3.22.0"
  },
  "peerDependencies": {
    "bs-platform": "^6.2"
  },
  "dependencies": {
    "@elliottcable/bs-cmdliner": "^10.2.2",
    "@google-cloud/storage": "^5.0.0",
    "@o1labs/client-sdk": "^0.1.2"
  }
}
